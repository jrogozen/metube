<div class="main large-12 columns">
  <h3>Users</h3>
  <ul class="user-list small-block-grid-4">
    <% @users.each do |user| %>
    <li>
      <div class="user-photo">
       <%= image_tag user.avatar_url(:thumb) %>
      </div>
      <div class="user-username">
        <h5><%= link_to user.username, user %></h5>
      </div>
    </li>
    <% end %>
  </ul>
</div>
