<%= form_for @video do |f| %>

  <% if @video.errors.any? %>
    <ul>
      <% @video.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  <% end %>

  title: <%= f.text_field :title %><br>
  youtube id: <%= f.text_field :url %><br>
  user: <%= f.select(:user_id, @user_options) %>
  description: <%= f.text_area :description %>
  <%= f.submit %>
<% end %>